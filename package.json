{
	"name": "next-react-app",
	"version": "4.0.0",
	"private": true,
	"description": "Next React App Template",
	"keywords": [
		"react",
		"next-react-app",
		"typescript"
	],
	"bugs": {
		"url": "https://github.com/timelessco/next-react-app/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/timelessco/next-react-app.git"
	},
	"license": "MIT",
	"author": "Navin Moorthy <navin007.a@gmail.com>",
	"type": "module",
	"scripts": {
		"prepare": "husky install",
		"dev": "next dev",
		"dev:sourcemap": "SOURCEMAP=true pnpm dev",
		"build": "next build",
		"postbuild": "next-sitemap --config next-sitemap.config.cjs",
		"build:analyze": "ANALYZE=true pnpm build && pnpm start",
		"build:ci": "SKIP_ENV_VALIDATION=true next build",
		"build:sourcemap": "SOURCEMAP=true pnpm build && source-map-explorer .next/static/**/*.js && pnpm start",
		"start": "next start",
		"build:start": "pnpm build && pnpm start",
		"lint": "npm-run-all lint:*",
		"lint:prettier": "prettier --check --cache --cache-location=.prettiercache --ignore-unknown .",
		"lint:eslint": "eslint --cache --color .",
		"lint:types": "tsc --noEmit",
		"lint:css": "stylelint '**/*.css' --cache --color -f verbose",
		"lint:md": "markdownlint **/*.md \".github/**/*.md\"",
		"lint:package-json": "npmPkgJsonLint .",
		"lint:spelling": "cspell --dot --gitignore --cache --no-progress check \"**/*\"",
		"format": "npm-run-all format:*",
		"format:prettier": "prettier --write --cache --cache-location=.prettiercache --list-different --ignore-unknown .",
		"format:eslint": "eslint --cache --color . --fix",
		"format:css": "stylelint '**/*.css' --fix --cache --color -f verbose",
		"format:md": "markdownlint --fix **/*.md \".github/**/*.md\"",
		"populate:dictionary": "del-cli project-words.txt && cspell --words-only --unique --gitignore --cache --dot \"**/*\" | sort --ignore-case >> project-words.txt",
		"release": "node release-it/getCommitsSinceLastRelease.js && release-it",
		"release:dryrun": "release-it --dry-run",
		"test": "echo \"no test specified\" && exit 0",
		"commit": "gacp"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": [
			"pnpm format:prettier",
			"pnpm lint:spelling"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"next": "13.2.4",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.5.1",
		"@commitlint/config-conventional": "17.4.4",
		"@ianvs/prettier-plugin-sort-imports": "3.7.2",
		"@next/bundle-analyzer": "13.2.4",
		"@next/env": "13.2.4",
		"@next/eslint-plugin-next": "13.2.4",
		"@octokit/core": "4.2.0",
		"@release-it/conventional-changelog": "5.1.1",
		"@types/node": "18.15.11",
		"@types/react": "18.0.31",
		"@types/react-dom": "18.0.11",
		"@typescript-eslint/experimental-utils": "5.57.0",
		"autoprefixer": "10.4.14",
		"cspell": "6.31.1",
		"dedent": "0.7.0",
		"del-cli": "5.0.0",
		"dotenv": "16.0.3",
		"eslint": "8.37.0",
		"eslint-config-canonical": "41.0.1",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-jsonc": "2.7.0",
		"eslint-plugin-tailwindcss": "3.10.1",
		"eslint-plugin-toml": "0.4.0",
		"eslint-plugin-yml": "1.5.0",
		"execa": "7.1.1",
		"gacp": "3.0.3",
		"git-remote-origin-url": "4.0.0",
		"git-url-parse": "13.1.0",
		"husky": "8.0.3",
		"jsonc-eslint-parser": "2.2.0",
		"knip": "2.2.0",
		"lint-staged": "13.2.0",
		"markdownlint": "0.28.0",
		"markdownlint-cli": "0.33.0",
		"next-bundle-analyzer": "0.6.7",
		"next-sitemap": "4.0.6",
		"npm-package-json-lint": "6.4.0",
		"npm-package-json-lint-config-default": "5.0.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"prettier": "2.8.7",
		"prettier-plugin-packagejson": "2.4.3",
		"prettier-plugin-tailwindcss": "0.2.5",
		"release-it": "15.9.3",
		"sharp": "0.32.0",
		"source-map-explorer": "2.5.3",
		"stylelint": "15.3.0",
		"stylelint-config-clean-order": "5.0.0",
		"stylelint-config-standard": "31.0.0",
		"tailwindcss": "3.3.0",
		"tilg": "0.1.1",
		"toml-eslint-parser": "0.5.0",
		"typescript": "5.0.2",
		"yaml-eslint-parser": "1.2.0",
		"zod": "3.21.4"
	},
	"packageManager": "pnpm@8.0.0",
	"gacp": {
		"add": false,
		"push": false,
		"emoji": "emoji",
		"editor": false
	}
}
